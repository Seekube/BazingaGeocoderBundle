{
  "name": "willdurand/geocoder-bundle",
  "description": "Integration of Geocoder into Symfony2",
  "keywords": ["geocoding", "geocoder"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "William Durand",
      "email": "william.durand1@gmail.com"
    }
  ],
  "require": {
    "symfony/framework-bundle": ">=2.1,<2.3-dev",
    "willdurand/geocoder": "*",
    "symfony/console": ">=2.1,<2.3-dev"
  },
  "require-dev": {
    "symfony/yaml": ">=2.1,<2.3-dev"
  },
  "autoload": {
    "psr-0": { "Bazinga\\Bundle\\GeocoderBundle": "" }
  },
  "target-dir": "Bazinga/Bundle/GeocoderBundle",
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
